{
    "name": "crondog/crondog-php",
    "description": "CronDog PHP API Client",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "illuminate/support": "~5.1|~5.2|~5.3",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0@dev",
        "phpunit/phpunit": "~4.0",
        "php-vcr/php-vcr": "^1.3",
        "php-vcr/phpunit-testlistener-vcr": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "CronDog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CronDog\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
